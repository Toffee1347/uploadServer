{
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"google"
	],
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"root": true,
	"rules": {
        "indent": ["warn", "tab"],
		"require-jsdoc": "off",
		"max-len": ["warn", {"code": 120}],
		"linebreak-style": "off",
		"no-tabs": "off",
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": "warn"
	}
}